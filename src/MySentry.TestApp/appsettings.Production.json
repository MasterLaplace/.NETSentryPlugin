{
  "MySentry": {
    "Dsn": "",
    "Enabled": true,
    "Environment": "Production",
    "Debug": false,
    "DiagnosticLevel": "Warning",
    "SendDefaultPii": false,
    "AttachStacktrace": true,
    "MaxBreadcrumbs": 50,
    "SampleRate": 1.0,
    "MinimumBreadcrumbLevel": "Warning",
    "MinimumEventLevel": "Error",
    "ShutdownTimeout": "00:00:05",
    "Tracing": {
      "Enabled": true,
      "SampleRate": 0.2,
      "TraceAllRequests": true,
      "TraceDatabase": true,
      "TraceHttpClients": true,
      "IgnoreUrls": [
        "/health",
        "/health/*",
        "/ready",
        "/swagger/*",
        "/_framework/*",
        "/favicon.ico"
      ]
    },
    "Profiling": {
      "Enabled": true,
      "SampleRate": 0.1
    },
    "Filtering": {
      "IgnoreExceptionTypes": [
        "System.OperationCanceledException",
        "System.Threading.Tasks.TaskCanceledException",
        "Microsoft.AspNetCore.Http.BadHttpRequestException"
      ],
      "IgnoreStatusCodes": [400, 401, 403, 404],
      "IgnoreUserAgents": [
        "health*",
        "kube-probe/*",
        "GoogleHC/*",
        "ELB-HealthChecker/*"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Sentry": "Warning"
    }
  }
}
